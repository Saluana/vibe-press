{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "bun run src/server.ts",
"migrate:generate": "drizzle-kit generate",
    "migrate:up": "drizzle-kit push"
  },
  "drizzle": {
